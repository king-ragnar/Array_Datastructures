'''

SIMPLE LINKED LIST WITH PRINTING


'''


#Node has data and next pointing to another Node

class Node:
    
    def __init__(self,data):
        self.data = data
        self.next = None

#list has head pointer which points to the node 
class LinkedList:
    
    def __init__(self):
        self.head = None
    
    def printl(self):
        temp = self.head
        while ( temp is not None):
            print(temp.data,end = " ")
            temp= temp.next

l1 = LinkedList()
n1 = Node(1)
n2 = Node(2)
n3 = Node(3)

n1.next = n2
l1.head =n1
n2.next = n3
#l1.head.next = n2
l1.printl()
