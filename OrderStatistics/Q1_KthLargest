
def Largest(arr,n,key):
    for i in range(n):
        count=0
        for j in range(n):  
            if arr[j] < arr[i]:
                count+=1
        if (n-count == key):
            print(arr[i])
# Driver Code 


if __name__=='__main__': 
    arr = [ 7, 10, 4, 3, 20, 15] 
    n = len(arr) 
    key = 2
    Largest(arr,n,key) 
