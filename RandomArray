import random 
  
# A function to generate a random permutation of arr[]




def randomize (arr, n): 
    for i in range(n):
        rand=random.randint(0,len(arr)-1)
        arr[i],arr[rand]=arr[rand],arr[i]
    return arr
  
# Driver program to test above function. 
arr = [1, 2, 3, 4, 5, 6, 7, 8] 
n = len(arr) 
print(randomize(arr, n)) 
